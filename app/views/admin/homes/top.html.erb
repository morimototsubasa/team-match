<h1>会員一覧</h1>
<table class="table">
  <thead>
    <tr>
      <th>氏名</th>
      <th>メールアドレス</th>
      <th>学校名</th>
    </tr>
  </thead>
  <tbody>
    <% @customers.each do |customer| %>
    <tr>
      <td><%= customer.name %></td>
      <td><%= customer.email %></td>
      <td><%= customer.school_name %></td>
    </tr>
    <% end %>
  </tbody>
</table>
