<div class="container mt-5 col-md-3">
  <div class="row">
    <h2>種目編集</h2>
    <%= form_with model:@genre, url:admin_genres_path, method: :post do |f| %>
    <p>種目名</p>
    <%= f.text_field :genre %>
    <%= f.submit '更新' ,class:"mr-3"%><%=link_to "削除", admin_genre_path(@genre.id), method: :delete %>
    <% end %>
  </div>
</div>